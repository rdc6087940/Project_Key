$(function(){
	
	
	let buy=$('#buy');
	
	buy.click(function(){
		location.href = 'end.html';
	})
	
	let btn = $('#notweek');
	let btn2 = $('#week');
	let ctn = 0;
	
	btn.click(function(){
		$(this).css('background-color','#8A6D3B');
		if(ctn==2){
			btn2.css('background-color','');
		}
		ctn=1;
	});
	
	btn2.click(function(){
		$(this).css('background-color','#8A6D3B');
		if(ctn==1){
			btn1.css('background-color','');
		}
		ctn=2;
	})
	
});